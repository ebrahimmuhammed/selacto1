@model IEnumerable<MESS.Models.User>
    

<h1>Product Lists</h1>



<table class="table">
    <thead>
        <tr>
            <th>Product ID</th>
            <th>Product Name</th>
            <th>Product Category</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.productId</td>
            <td>@user.productName</td>
            <td>@user.productCategory</td>
        </tr>
    }
    </tbody>
</table>
<a asp-action="Create" class="btn btn-primary">Add New Product</a>